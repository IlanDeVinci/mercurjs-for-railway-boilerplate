{
  "name": "vendor-panel",
  "version": "1.0.0",
  "scripts": {
    "generate:static": "node ./scripts/generate-currencies.js && prettier --write ./src/lib/currencies.ts",
    "wait": "await-backend",
    "dev": "pnpm run wait && node ./scripts/launch-vendor.js",
    "start": "pnpm run wait && node ./scripts/launch-vendor.js",
    "build": "vite build && tsup && node ./scripts/generate-types.js",
    "build:preview": "vite build",
    "build:admin": "medusa build",
    "preview": "vite preview",
    "test": "vitest --run",
    "i18n:validate": "node ./scripts/i18n/validate-translation.js",
    "i18n:schema": "node ./scripts/i18n/generate-schema.js",
    "lint": "eslint src --ext ts,tsx,js,jsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "main": "lib/app.js",
  "module": "lib/app.mjs",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/app.mjs",
      "require": "./lib/app.js"
    },
    "./css": {
      "import": "./lib/app.css",
      "require": "./lib/app.css"
    },
    "./root": "./",
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://https://github.com/mercurjs/vendor-panel"
  },
  "files": [
    "package.json",
    "src",
    "index.html",
    "dist",
    "lib"
  ],
  "dependencies": {
    "@ariakit/react": "^0.4.15",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "3.4.2",
    "@medusajs/admin-shared": "^2.13.1",
    "@medusajs/icons": "^2.13.1",
    "@medusajs/js-sdk": "^2.13.1",
    "@medusajs/ui": "~4.1.1",
    "@tanstack/react-query": "5.90.20",
    "@tanstack/react-table": "8.21.3",
    "@tanstack/react-virtual": "^3.13.18",
    "@uiw/react-json-view": "^2.0.0-alpha.17",
    "cmdk": "^0.2.0",
    "copy-to-clipboard": "^3.3.3",
    "date-fns": "^3.6.0",
    "eslint-nibble": "^9.1.1",
    "i18next": "23.7.11",
    "i18next-browser-languagedetector": "7.2.0",
    "i18next-http-backend": "2.4.2",
    "lodash": "^4.17.23",
    "match-sorter": "^8.2.0",
    "medusajs-launch-utils": "^0.0.19",
    "motion": "^11.15.0",
    "qs": "^6.12.0",
    "radix-ui": "1.4.3",
    "react": "^18.2.0",
    "react-country-flag": "^3.1.0",
    "react-currency-input-field": "^3.6.11",
    "react-day-picker": "^9.13.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "7.71.1",
    "react-i18next": "13.5.0",
    "react-jwt": "^1.2.0",
    "react-router-dom": "6.20.1",
    "recharts": "^2.15.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@medusajs/admin-shared": "^2.13.1",
    "@medusajs/admin-vite-plugin": "^2.13.1",
    "@medusajs/types": "^2.13.1",
    "@medusajs/ui-preset": "^2.13.1",
    "@types/lodash": "^4.17.23",
    "@types/node": "^20.11.15",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitejs/plugin-react": "4.2.1",
    "ajv": "^8.17.1",
    "autoprefixer": "^10.4.24",
    "eslint": "^9.39.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "postcss": "^8.4.33",
    "prettier": "^3.8.1",
    "tailwindcss": "^3.4.1",
    "tsup": "^8.0.2",
    "typescript": "5.9.3",
    "vite": "^5.4.14",
    "vite-plugin-inspect": "^0.8.7",
    "vitest": "^3.0.5"
  }
}
