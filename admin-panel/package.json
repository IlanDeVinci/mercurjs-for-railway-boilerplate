{
  "name": "@medusajs/dashboard",
  "version": "2.10.3",
  "scripts": {
    "generate:static": "node ./scripts/generate-currencies.js && prettier --write ./src/lib/currencies.ts",
    "dev": "vite",
    "build": "tsup && vite build && node ./scripts/generate-types.js",
    "build:preview": "vite build",
    "preview": "vite preview",
    "start": "vite",
    "test": "vitest --run",
    "i18n:validate": "node ./scripts/i18n/validate-translation.js",
    "i18n:schema": "node ./scripts/i18n/generate-schema.js",
    "lint": "eslint src --ext ts,tsx,js,jsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "main": "dist/app.js",
  "module": "dist/app.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/app.mjs",
      "require": "./dist/app.js",
      "types": "./dist/index.d.ts"
    },
    "./css": {
      "import": "./dist/app.css",
      "require": "./dist/app.css"
    },
    "./root": "./",
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/medusajs/medusa",
    "directory": "packages/admin/dashboard"
  },
  "files": [
    "package.json",
    "src",
    "dist"
  ],
  "dependencies": {
    "@ariakit/react": "^0.4.15",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "3.4.2",
    "@medusajs/admin-shared": "2.13.1",
    "@medusajs/icons": "2.13.1",
    "@medusajs/js-sdk": "2.13.1",
    "@medusajs/ui": "4.1.1",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-dismissable-layer": "1.1.11",
    "@tanstack/react-query": "5.90.20",
    "@tanstack/react-table": "8.21.3",
    "@tanstack/react-virtual": "^3.13.18",
    "@uiw/react-json-view": "^2.0.0-alpha.17",
    "cmdk": "^0.2.0",
    "copy-to-clipboard": "^3.3.3",
    "date-fns": "^3.6.0",
    "i18next": "23.7.11",
    "i18next-browser-languagedetector": "7.2.0",
    "i18next-http-backend": "2.4.2",
    "lodash": "^4.17.23",
    "match-sorter": "^8.2.0",
    "motion": "^11.15.0",
    "qs": "^6.14.1",
    "radix-ui": "1.4.3",
    "react": "^18.2.0",
    "react-country-flag": "^3.1.0",
    "react-currency-input-field": "^3.6.11",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "7.71.1",
    "react-i18next": "13.5.0",
    "react-jwt": "^1.2.0",
    "react-router-dom": "6.20.1",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@medusajs/admin-shared": "2.13.1",
