{
  "name": "medusa-starter-default",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build",
    "ib": "init-backend",
    "init-meili": "medusa exec ./src/scripts/init-meili.ts",
    "reindex-meili": "medusa exec ./src/scripts/reindex-meili.ts",
    "start": "init-backend && pnpm exec medusa db:migrate && if [ \"${SEED_DB:-false}\" = \"true\" ]; then pnpm seed; fi && pnpm run init-meili && medusa start",
    "dev": "medusa develop",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "test:integration:http": "cross-env TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules --dns-result-order=ipv4first jest --silent=false --runInBand --forceExit --passWithNoTests",
    "test:integration:modules": "cross-env TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules --dns-result-order=ipv4first jest --silent=false --runInBand --forceExit --passWithNoTests",
    "test:unit": "cross-env TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules --dns-result-order=ipv4first jest --silent --runInBand --forceExit --passWithNoTests",
    "lint": "eslint src --ext ts,tsx,js,jsx --max-warnings 0"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "2.13.1",
    "@medusajs/cli": "2.13.1",
    "@medusajs/dashboard": "2.13.1",
    "@medusajs/draft-order": "2.13.1",
    "@medusajs/framework": "2.13.1",
    "@medusajs/icons": "2.13.1",
    "@medusajs/medusa": "2.13.1",
    "@medusajs/ui": "4.1.1",
    "@mercurjs/b2c-core": "^1.5.1",
    "@mercurjs/commission": "^1.5.1",
    "@mercurjs/framework": "^1.5.1",
    "@mercurjs/payment-stripe-connect": "^1.5.1",
    "@mercurjs/requests": "^1.5.1",
    "@mercurjs/resend": "^1.5.1",
    "@mercurjs/reviews": "^1.5.1",
    "@mikro-orm/core": "6.4.16",
    "@mikro-orm/knex": "6.4.16",
    "@mikro-orm/migrations": "6.4.16",
    "@mikro-orm/postgresql": "6.4.16",
    "awilix": "^8.0.1",
    "medusajs-launch-utils": "^0.0.19",
    "meilisearch": "^0.45.0",
    "minio": "^8.0.3",
    "pg": "^8.18.0",
    "posthog-node": "^5.24.9",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@medusajs/test-utils": "2.13.1",
    "@mikro-orm/cli": "6.4.16",
    "@swc/core": "1.15.11",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.39.2",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.2.11",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=20"
  }
}
